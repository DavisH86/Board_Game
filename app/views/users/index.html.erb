
<% provide(:title, 'All users') %>
<div class="description">
  <h1>All Users</h1>

<%= will_paginate @users %>

<ul class="users">
  <% @users.each do |user|%>
    <li>
      <%= (image_tag "#{user.profile_photo}", size: 52) %>
      <%= link_to "#{user.name}", user_path(user)%>
    </li>
  <% end %>
</ul>

<%= will_paginate @users%>

</div>
